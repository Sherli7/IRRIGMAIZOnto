    <rdf:Description rdf:about="http://www.w3.org/2002/07/etp">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/s">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/wc">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/it">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/irrigationEvent">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/sm">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/moisture">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/st">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/tension">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.w3.org/2002/07/threshold">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <ns2:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ns2:isRuleEnabled>
        <rdfs:comment>Règle combinée pour déclencher l&apos;irrigation en fonction de l&apos;humidité et de la tension</rdfs:comment>
        <rdfs:label>IBHT</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.example.org/irrigation#SoilMoisture"/>
                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/sm"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.example.org/irrigation#moistureLevel"/>
                                <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/sm"/>
                                <swrl:argument2 rdf:resource="http://www.w3.org/2002/07/moisture"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                        <swrl:classPredicate rdf:resource="http://www.example.org/irrigation#SoilTension"/>
                                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/st"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="http://www.example.org/irrigation#tensionLevel"/>
                                                <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/st"/>
                                                <swrl:argument2 rdf:resource="http://www.w3.org/2002/07/tension"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                                        <swrl:classPredicate rdf:resource="http://www.example.org/irrigation#IrrigationThreshold"/>
                                                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/it"/>
                                                    </rdf:Description>
                                                </rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                        <rdf:first>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                                                <swrl:propertyPredicate rdf:resource="http://www.example.org/irrigation#thresholdValue"/>
                                                                <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/it"/>
                                                                <swrl:argument2 rdf:resource="http://www.w3.org/2002/07/threshold"/>
                                                            </rdf:Description>
                                                        </rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                                <rdf:first>
                                                                    <rdf:Description>
                                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                                                        <swrl:classPredicate rdf:resource="http://www.example.org/irrigation#Irrigation"/>
                                                                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/irrigationEvent"/>
                                                                    </rdf:Description>
                                                                </rdf:first>
                                                                <rdf:rest>
                                                                    <rdf:Description>
                                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                                        <rdf:first>
                                                                            <rdf:Description>
                                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#BuiltinAtom"/>
                                                                                <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#lessThan"/>
                                                                                <swrl:arguments rdf:parseType="Collection">
                                                                                    <rdf:Description rdf:about="http://www.w3.org/2002/07/moisture"/>
                                                                                    <rdf:Description rdf:about="http://www.w3.org/2002/07/threshold"/>
                                                                                </swrl:arguments>
                                                                            </rdf:Description>
                                                                        </rdf:first>
                                                                        <rdf:rest>
                                                                            <rdf:Description>
                                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                                                <rdf:first>
                                                                                    <rdf:Description>
                                                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#BuiltinAtom"/>
                                                                                        <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#greaterThan"/>
                                                                                        <swrl:arguments rdf:parseType="Collection">
                                                                                            <rdf:Description rdf:about="http://www.w3.org/2002/07/tension"/>
                                                                                            <rdf:Description rdf:about="http://www.w3.org/2002/07/threshold"/>
                                                                                        </swrl:arguments>
                                                                                    </rdf:Description>
                                                                                </rdf:first>
                                                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                                            </rdf:Description>
                                                                        </rdf:rest>
                                                                    </rdf:Description>
                                                                </rdf:rest>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="http://www.example.org/irrigation#triggersIrrigation"/>
                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/it"/>
                        <swrl:argument2 rdf:resource="http://www.w3.org/2002/07/irrigationEvent"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
    <rdf:Description>
        <ns2:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ns2:isRuleEnabled>
        <rdfs:comment>Règle pour ajuster l&apos;irrigation en fonction de l&apos;ETP et des précipitations</rdfs:comment>
        <rdfs:label>IBETPP</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.example.org/irrigation#Evapotranspiration"/>
                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/etp"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://www.example.org/irrigation#hasEvapotranspiration"/>
                                <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/s"/>
                                <swrl:argument2 rdf:resource="http://www.w3.org/2002/07/etp"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                        <swrl:classPredicate rdf:resource="http://www.example.org/irrigation#WeatherCondition"/>
                                        <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/wc"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="http://www.example.org/irrigation#triggersIrrigation"/>
                                                <swrl:argument1 rdf:resource="http://www.w3.org/2002/07/it"/>
                                                <swrl:argument2 rdf:resource="http://www.w3.org/2002/07/irrigationEvent"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
